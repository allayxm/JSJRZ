@model MXKJ.JSJRZ.WebUI.Models.PeerResponse.StatisticsOrgListViewModel
@{
    ViewBag.Title = "学生互评列表";
    Layout = "~/Views/Shared/_LayoutPeerResponse.cshtml";
}
<body class="hold-transition sidebar-mini" s style="background-color:#ecf0f5;">
    <div class="wrapper">
        <!-- Content Wrapper. Contains page content -->
        <div>
            <!-- Content Header (Page header) -->
            <section class="content-header text-center">
                <h1>
                    学生互评列表
                </h1>
            </section>
            <!-- Main content -->
            <section class="content">
                @using (Html.BeginForm("StatisticsOrgList", "PeerResponse", FormMethod.Post, new { role = "form" }))
                {
                    <div class="row">
                        <!-- left column -->
                        <div class="col-md-12">
                            <!-- general form elements -->
                            <div class="box box-primary">
                                <!-- form start -->
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="box-body">
                                            <table class="table table-bordered table-striped">
                                                <thead>
                                                    <tr>
                                                        <th>班级列表</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @for (int i = 0; i < Model.ClassNameArray.Length; i++)
                                                    {
                                                        <tr class="even gradeA">
                                                            <td><a href="~/PeerResponse/StatisticsOrgScore/@Model.ClassIDArray[i]">@Model.ClassNameArray[i]</a></td>
                                                        </tr>
                                                    }
                                            </table>
                                        </div><!-- /.box-body -->
                                    </div>
                                </div>
                            </div><!-- /.box -->
                        </div><!--/.col (left) -->
                    </div>   <!-- /.row -->
                }
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
    </div><!-- ./wrapper -->
</body>

